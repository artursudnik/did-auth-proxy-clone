{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "labels": [
    "Renovate Bot"
  ],
  "rangeStrategy": "pin",
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "groupName": "Auth service",
      "matchFileNames": [
        "authorization-server/**"
      ],
      "labels": [
        "autorization service"
      ]
    }
  ]
}
